<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <staticContent>
            <remove fileExtension=".xml" />
            <mimeMap fileExtension=".xml" mimeType="application/rss+xml" />
        </staticContent>
        <defaultDocument>
            <files>
                <add value="index.xml" />
            </files>
        </defaultDocument>
    </system.webServer>
</configuration>
